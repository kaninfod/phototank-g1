#photogrid
  .col-lg-12.col-sm-12
    .box.box-solid
      .box-header.with-border
        %ul.text-right.list-inline
          - if defined? extra_tools.nil?
            %li= render :partial => extra_tools

      .box-body.infinite-container
        =render :partial => "photos/view/grid"
      .loading-notification
        Loading more...
      .loadMore

  #photoDetails.animated.animated-modal
    .modal-content
    .close-modal.close-modal Close
